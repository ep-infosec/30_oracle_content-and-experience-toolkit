      <spec>H/15 * * * *</spec>
      <ignorePostCommitHooks>false</ignorePostCommitHooks>
    </hudson.triggers.SCMTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>#!/bin/bash -x

cd ./SitesCloud/test
source setup.sh

# ---------------------------------

echo &apos;fixing up lfs files&apos;
cd ../../developer
git lfs install
sleep 5
rm sites-toolkit/test/sitescloud/renderer/renderer.js
git checkout sites-toolkit/test/sitescloud/renderer/renderer.js
rm test/sites-compiler/test/data/siteCompileTestTemplate.zip
git checkout test/sites-compiler/test/data/siteCompileTestTemplate.zip


echo &apos;installing sites-toolkit...&apos;
cd ./sites-toolkit
npm install
npm install bin/cec

export PATH=`pwd`/node_modules/.bin:$PATH
which cec


echo &apos;installing sites-compiler test folder...&apos;
cd ../test/sites-compiler
npm install

echo &apos;installing cec install...&apos;
npm run cec-install

echo &apos;starting the test server...&apos;
npm run start-server

echo &apos;create the starter template...&apos;
cd cec-install
cec create-template MyStarterTemplate -f StarterTemplate

echo &apos;run the unit tests...&apos;
npm run test-unit</command>
    </hudson.tasks.Shell>
