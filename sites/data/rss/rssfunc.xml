<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0">
<channel>
 <title>{{title}}</title>
 <description>{{description}}</description>
 <link>{{{url}}}</link>
 <lastBuildDate>{{lastBuildDate}} </lastBuildDate>
 <pubDate>{{pubDate}}</pubDate>
 <ttl>{{ttl}}</ttl>

{{#items}}

<item>
  <title>{{name}}</title>
  <description>{{description}}</description>
  <link>{{{detailPageUrl}}}?slug={{slug}}</link>
  <guid isPermaLink="false">{{id}}</guid>
  <pubDate>{{#rssDate}}{{fields.birthday.value}}{{/rssDate}}</pubDate>
 </item>

{{/items}}

</channel>
</rss>

